<p id="notice"><%= notice %></p>

<h1>Places</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id='places'>
    <%= render @places %>
  </tbody>
</table>

<br>

<%= link_to 'New Place', new_place_path %>

<%= content_tag :button, 'Get Places', method: :get, class: 'btn btn-info get_location' %>
<%= tag :div,  class: 'location_results_list' %>
